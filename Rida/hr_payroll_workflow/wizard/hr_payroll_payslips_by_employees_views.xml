<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="view_hr_payslip_by_employees_wizard" model="ir.ui.view">
            <field name="name">hr_payroll_payslip_employees_wizard</field>
            <field name="model">hr.payslip.employees</field>
            <field name="inherit_id" ref="hr_payroll.view_hr_payslip_by_employees"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='compute_sheet']" position="replace">
                    <button icon="fa-cogs" string="Generate" name="compute_sheet" type="object" class="oe_highlight"/>
                </xpath>
            </field>
        </record>

</odoo>
