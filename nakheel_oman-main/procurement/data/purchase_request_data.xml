        </record>

        <record id="purchase.seq_purchase_order" model="ir.sequence">
            <field name="name">RFQ</field>
            <field name="code">purchase.order</field>
            <field name="prefix">RFQ-</field>
            <field name="padding">3</field>
        </record>
    </data>

</odoo>
